 * 2. Inspect to bring up an Object Inspector on the result (Ctrl+I), or,
 * 3. Display to insert the result in a comment after the selection. (Ctrl+L)
 */

var f=function(){
  for(var i=0;i<arguments.length;i++)
    {
      
      var n=arguments[i];
      console.log(n);
      var mod=n%10;
      var div=parseInt(n/10);
      var pro=n*mod;
      var sum=mod.toString()+div.toString();
      if(pro==parseInt(sum))
        console.log("circular Number");
      else
        console.log("Not a circular number");
    }
}
f(102564,111111,543678);