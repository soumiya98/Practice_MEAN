function counter()
{
  var count=30;
  var obj={
    increment:function(){
    count++;
  },
      decrement:function(){
        count--;
      },
        getcount:function(){
          return count;
        },
    modify:function(co){
      count=count+co;
    }
};
return obj;
  
   
  
}

var count1=counter();
console.log(count1.increment());
console.log(count1.decrement());
console.log(count1.getcount());
console.log(count1.modify(-1));
console.log(count1.getcount());
console.log(count1.count);


